y = Integer.parseInt(stack.pop());
value = y - x;
result = p + value;
stack.push(result);
break;

//case for ‘*’
case "*":
x = Integer.parseInt(stack.pop());
y = Integer.parseInt(stack.pop());
value = x * y;
result = p + value;
stack.push(result);
break;

//case for ‘/’
case "/":
x = Integer.parseInt(stack.pop());
y = Integer.parseInt(stack.pop());
value = y / x;
result = p + value;
stack.push(result);
break;
default:

continue;
}
  }
return Integer.parseInt(stack.pop());
}
